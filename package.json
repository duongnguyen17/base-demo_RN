{
  "name": "base",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "makeIcons": "node scripts/icon.js",
    "prettier": "./node_modules/.bin/prettier --write src/**",
    "android:dev": "react-native run-android --variant=developmentdebug",
    "android:staging": "react-native run-android --variant=stagingdebug",
    "android:production": "react-native run-android --variant=productiondebug",
    "web:dev": "node ./scripts/generate_web_config.js env:.env.development && npx expo start --web --https",
    "web:staging": "node ./scripts/generate_web_config.js env:.env.staging && npx expo start --web --https",
    "web:production": "node ./scripts/generate_web_config.js env:.env.production && npx expo start --web --https",
    "web:dev:build": "node ./scripts/generate_web_config.js env:.env.development && npx expo export:web ",
    "web:staging:build": "node ./scripts/generate_web_config.js env:.env.staging && npx expo export:web",
    "web:production:build": "node ./scripts/generate_web_config.js env:.env.production && npx expo export:web",
    "android:dev:build": "react-native run-android --variant=developmentrelease",
    "android:staging:build": "react-native run-android --variant=stagingrelease",
    "android:production:build": "react-native run-android --variant=productionrelease"
  },
  "dependencies": {
    "@babel/plugin-proposal-export-namespace-from": "^7.18.9",
    "@expo/webpack-config": "^0.17.0",
    "@hookform/resolvers": "^2.9.7",
    "@react-native-community/blur": "^4.2.0",
    "@react-navigation/bottom-tabs": "^6.3.2",
    "@react-navigation/native": "^6.0.11",
    "@react-navigation/native-stack": "^6.7.0",
    "@react-navigation/stack": "^6.2.2",
    "@shopify/flash-list": "^1.2.1",
    "dayjs": "^1.11.4",
    "expo": "^46.0.0",
    "expo-blur": "^11.2.0",
    "expo-linear-gradient": "~11.4.0",
    "expo-secure-store": "~11.3.0",
    "i18next": "^21.8.16",
    "lodash": "^4.17.21",
    "native-base": "^3.4.11",
    "path-to-regexp": "^6.2.1",
    "pluralize": "^8.0.0",
    "qs": "^6.11.0",
    "react": "18.0.0",
    "react-dom": "18.0.0",
    "react-hook-form": "^7.34.0",
    "react-i18next": "^11.18.3",
    "react-native": "0.69.4",
    "react-native-bootsplash": "^4.3.2",
    "react-native-config": "^1.4.6",
    "react-native-fast-image": "^8.5.11",
    "react-native-gesture-handler": "~2.5.0",
    "react-native-image-colors": "^1.5.1",
    "react-native-mmkv": "^2.4.3",
    "react-native-reanimated": "^2.9.1",
    "react-native-reanimated-carousel": "^3.0.3",
    "react-native-redash": "^18.0.0",
    "react-native-safe-area-context": "4.3.1",
    "react-native-screens": "~3.15.0",
    "react-native-shared-element": "^0.8.4",
    "react-native-svg": "12.3.0",
    "react-native-vector-icons": "^9.2.0",
    "react-native-web": "~0.17.7",
    "react-navigation-shared-element": "^3.1.3",
    "recoil": "^0.7.5",
    "reselect": "^4.1.6",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/runtime": "^7.12.5",
    "@react-native-community/eslint-config": "^2.0.0",
    "@tsconfig/react-native": "^2.0.2",
    "@types/jest": "^26.0.23",
    "@types/lodash": "^4.14.182",
    "@types/pluralize": "^0.0.29",
    "@types/qs": "^6.9.7",
    "@types/react-native": "^0.69.3",
    "@types/react-native-snap-carousel": "^3.8.5",
    "@types/react-native-vector-icons": "^6.4.11",
    "@types/react-test-renderer": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^5.29.0",
    "@typescript-eslint/parser": "^5.29.0",
    "babel-jest": "^26.6.3",
    "babel-plugin-module-resolver": "^4.1.0",
    "babel-preset-expo": "^9.1.0",
    "eslint": "^7.32.0",
    "jest": "^26.6.3",
    "metro-react-native-babel-preset": "^0.70.3",
    "prettier": "^2.7.1",
    "react-test-renderer": "18.0.0",
    "sharp": "^0.30.7",
    "typescript": "^4.4.4"
  },
  "expo": {
    "autolinking": {
      "exclude": [
        "expo-keep-awake",
        "expo-blur"
      ]
    }
  },
  "jest": {
    "preset": "react-native",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "setupFiles": [
      "<rootDir>/jest/setup.js"
    ]
  }
}
